<!--pages/list/list.wxml-->
<view class='date-item' wx:key wx:for="{{items}}">
  <view class='date-wrapper'>
    <view class='day'>{{item.day}}</view>
    <view class='date'>{{item.date}}</view>
  </view>
  <view class='temp'>{{item.minTemp}}°-{{item.maxTemp}}°</view>
  <image class='weather-icon' src='/images/{{item.weather}}-icon.png'></image>
</view>
